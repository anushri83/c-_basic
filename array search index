#include<iostream>
using namespace std;

int search(int A[],int n,int key)
{
    for(int i=0;i<n;i++)
        if(key==A[i])
        {
            return i;
        }
    return 0;
}
int main()
{
    int A[]={20,30,17,9,39,49};
    int k;
    cout<<"enter key to be search"<<endl;
    cin>>k;
    int index=search(A,7,k);
    cout<<"element found at index: "<<index<<endl;
}
