#include<iostream>
using namespace std;

int main()
{
    int x[5],n=5, key;
    cout<<"enter element: "<<endl;
    for(int i=0;i<=5;i++)
    {
        cin>>x[i];
    }

    cout<<"enter key to search: ";
    cin>>key;
    for(int i=0;i<n;i++)
    {
        if (key==x[i])
        {
           cout<<"key found at: "<<i<<endl;
        }
    }
      cout<<"key not found";
    return 0;
}
