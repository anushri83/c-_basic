
#include<iostream>
using namespace std;

int main()
{
    int age;
    cout<<"enter age: "<<endl;
    cin>>age;

    if(age>=18 && age<=50)
    {
        cout<<"young";
    }
    else if(age>0 && age<=17)
        {
        cout<<"child";
        }
        else
        {
            cout<<"old";
        }
    }
    return 0;
}
