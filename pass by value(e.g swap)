//pass by value
#include<iostream>                               
using namespace std;
int swap(int a,int b)         //formal value
{
    int temp;
    temp=a;
    a=b;
    b=temp;
    cout<<a<<" "<<b<<endl;
}
int main()
{
   int x=10,y=20;       //actual value(does not change)
   swap(x,y);
   cout<<x<<" "<<y<<endl;
   return 0; 
}
