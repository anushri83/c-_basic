#include<iostream>
using namespace std;

int main()
{
    int x=11,y=5,z1,z2,z3,z4,z5,z6; 
    
    z1=x&y;                    //(for AND )
    z2=x|y;                    //(for OR )
    z3=x^y;                    //(for EX-OR)
    z4=x<<1;                   //(for rightshift )        
    z5=x>>1;                   //(for left shift)
    z6=~x;                     //(for NOT)
    
    cout<<z1<<endl;
    cout<<z2<<endl;
    cout<<z3<<endl;
    cout<<z4<<endl;
    cout<<z5<<endl; 
    cout<<z6<<endl;  
    return 0;
}
